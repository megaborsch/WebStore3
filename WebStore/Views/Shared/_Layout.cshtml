
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title | E-Shopper</title>

    <link href="~/template/css/typography.css" rel="stylesheet" asp-append-version="true">
    <link href="~/template/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/template/css/font-awesome.min.css" rel="stylesheet" asp-append-version="true">
    <environment include="Development">
        <link href="~/template/css/prettyPhoto.css" rel="stylesheet">
        <link href="~/template/css/price-range.css" rel="stylesheet">
        <link href="~/template/css/animate.css" rel="stylesheet">
        <link href="~/template/css/main.css" rel="stylesheet">
        <link href="~/template/css/responsive.css" rel="stylesheet">
    </environment>
    <environment exclude="Development">
        <link href="~/template/css/site.min.css" rel="stylesheet" asp-append-version="true">
        
    </environment>
    @await RenderSectionAsync("css", false)
    <!--[if lt IE 9]>
    <script src="~/template/js/html5shiv.js">  </script>
    <script src="~/template/js/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="~/template/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/template/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/template/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/template/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/template/images/ico/apple-touch-icon-57-precomposed.png">
</head><!--/head-->
<body>
    @await Html.PartialAsync("_Header")


    <section>
        <div class="container">
            @RenderBody()
        </div>

    </section>
    @await Html.PartialAsync("_Footer")

    <script src="~/template/js/jquery.js" asp-append-version="true"></script>
    <script src="~/template/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/template/js/jquery.scrollUp.min.js" asp-append-version="true"></script>
    <environment include="Development">
        <script src="~/template/js/price-range.js"></script>
        <script src="~/template/js/jquery.prettyPhoto.js"></script>
        <script src="~/template/js/main.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/template/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @await RenderSectionAsync("scripts", false)
</body>
        </html>
